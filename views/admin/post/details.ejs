                        </div>
                    </div>
                    <div class="box-body">
                   <section class="invoice">
      <!-- title row -->
      <div class="row">
        <div class="col-xs-12">
          <h2 class="page-header">
            <label></label>
            <small class="pull-right">Date: <%=data.created_at %><br/></small>
          </h2>
		  </div>
        <!-- /.col -->
      </div>
      <!-- info row -->
      <div class="row invoice-info">
        <div class="col-sm-4 invoice-col">
          From
          <address>
            <strong><%= (data.address!==undefined)?data.address:"---" %><br>
            Phone: <%= (data.mobile_number!==undefined)?data.mobile_number:"---" %><br>
            Email: <%= (data.email!==undefined)?data.email:"---" %>
          </address>
        </div>
        <!-- /.col -->
        <div class="col-sm-4 invoice-col">
          
        </div>
        <!-- /.col -->
        <div class="col-sm-4 invoice-col">
          <b>Invoice #007612</b><br>
          <br>
          
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

      <!-- Table row -->
      <div class="row">
        <div class="col-xs-12 table-responsive">
          <table class="table table-striped">



           
  <tr>
    <th>Ad Title:</th>
    <td><%= (data.title!==undefined)?data.title:"---" %></td>
  </tr>
  <tr>
    <th>Ad Type:</th>
    <td><%= (data.adtype!==undefined)?data.adtype:"---" %></td>
  </tr>
  <tr>
    <th>Price:</th>
    <td><%= (data.price!==undefined)?data.price:"---" %></td>
  </tr>
  
  <tr>
    <th>Promotion Type:</th>
    <td><%= (data.promotion!==undefined)?data.promotion:"---" %></td>
  </tr>
	
  <tr>
    <th>Website:</th>
    <td><%= (data.website!==undefined)?data.website:"---" %></td>
  </tr>

          </table>
		  <div class="row box box-success">
		  <div class="col-lg-12 box-header">
		  <span class="">Description</span>
		  </div>
		  </div>
		  <div class="box-body row">
		  <div class="col-lg-12">
		
    
     <%= (data.description!==undefined)?data.description:"---" %>
  
  
		  </div>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

     

      <!-- this row will not appear when printing -->
