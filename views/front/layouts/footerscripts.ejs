        <div class="modal-footer">
            Don't have an account ?
             <a href="#" id="signupmode"> Sign up</a>
      </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
</div>

<div class="modal fade" tabindex="-1" role="dialog" id="Forgetpassmodal">
    <div class="modal-dialog modal-signin" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="fa fa-times"></i></button>
                 <h4 class="modal-title">Forget Password</h4>
             </div>
            <div class="modal-body">
				<form action="#" method="get" name="forgetFrm" id="forgetFrm" >
                    <div class="form-group">
						<label for="exampleInputEmail1">Email address</label>
						<input class="form-control" type="text" name="email" placeholder="E-mail Address" required>
					</div>
					
					<div class="form-group remember">
                        <a href="#" id="loginopen" title="">Back to login ?</a>
                    </div>
                    <div class="btn-more">
                        <button id="submit" type="submit" class="btn btn--primary btn--rounded btn--block">Forget Password</</button>
                    </div>
                </form><!-- /form -->
			               
			</div>
		
        <div class="modal-footer">
            Don't have an account ?
             <a href="#" id="signupmode"> Sign up</a>
      </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
</div>
