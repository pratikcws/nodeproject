    }).on('hide.bs.tab', function () {
        destroy_owl($('#task-carousel2'));
    });

    $('a[href="#stuff"]').on('shown.bs.tab', function () {
        initialize_owl($('#task-carousel3'));
    }).on('hide.bs.tab', function () {
        destroy_owl($('#task-carousel3'));
    });

    $('a[href="#party"]').on('shown.bs.tab', function () {
        initialize_owl($('#task-carousel4'));
    }).on('hide.bs.tab', function () {
        destroy_owl($('#task-carousel4'));
    });

    $('a[href="#diffrent"]').on('shown.bs.tab', function () {
        initialize_owl($('#task-carousel5'));
    }).on('hide.bs.tab', function () {
        destroy_owl($('#task-carousel5'));
    });
});

function initialize_owl(el) {
    el.owlCarousel({
        loop: true,
        margin: 10,
        nav: false,
        autoplay:true,
        responsiveClass: true,
        responsive: {
            0: {
                items: 1,
                
            },
            600: {
                items: 2,
              
            },
            1000: {
                items: 3,               
                
            }
        }
    });
}

function destroy_owl(el) {
